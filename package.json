{
  "name": "my-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gatsby": "^2.15.28",
    "node-sass": "^7.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-instagram-embed": "^1.5.0",
    "react-instagram-feed": "^0.1.3-alpha",
    "react-router-dom": "^5.1.0",
    "react-scripts": "0.9.5",
    "react-twitter-embed": "^2.0.8"
  },
  "scripts": {
    "build-css": "node-sass src/styles -o src/styles",
    "watch-css": "npm run build-css && node-sass src/styles -o src/styles --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "npm-run-all": "^4.1.5"
  }
}
